<?xml version="1.0" encoding="utf-8"?>
<ScrollView
android:id="@+id/widget246"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_alignParentTop="true"
android:layout_alignParentLeft="true"
>
<LinearLayout
android:id="@+id/widget118"
android:orientation="vertical"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
>


<TextView
android:id="@+id/widget234"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text="Username"
>
</TextView>
<EditText
android:id="@+id/accountUsername"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:textSize="18sp"
android:singleLine="true"
>
</EditText>
<TextView
android:id="@+id/widget235"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Password"
>
</TextView>
<EditText
android:id="@+id/accountPassword"
android:password="true"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:textSize="18sp"
android:singleLine="true"
>
</EditText>
<TextView
android:id="@+id/widget236"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Gender"
>
</TextView>
<RadioGroup
android:id="@+id/accountGender"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<RadioButton
android:id="@+id/accountGenderMale"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Male"
>
</RadioButton>
<RadioButton
android:id="@+id/accountGenderFemale"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Female"
>
</RadioButton>
</RadioGroup>
<TextView
android:id="@+id/widget237"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Year of birth"
>
</TextView>
<EditText
android:id="@+id/accountDOB"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:textSize="18sp"
android:singleLine="true"
>
</EditText>
<TextView
android:id="@+id/widget238"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Occupation"
>
</TextView>
<EditText
android:id="@+id/accountOccupation"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:textSize="18sp"
android:singleLine="true"
>
</EditText>
<TextView
android:id="@+id/widget301"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="E-Mail"
>
</TextView>
<EditText
android:id="@+id/accountEmail"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:textSize="18sp"
android:singleLine="true"
>
</EditText>
<Button
android:id="@+id/next_button"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text="Next"
>
</Button>
</LinearLayout>
</ScrollView>