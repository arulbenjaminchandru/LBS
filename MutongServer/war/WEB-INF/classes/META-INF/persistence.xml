<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
        http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">

	<persistence-unit name="mutong">
		<provider>org.datanucleus.store.appengine.jpa.DatastorePersistenceProvider</provider>
			
			<class>cn.edu.nju.software.gof.entity.Account</class>
			<class>cn.edu.nju.software.gof.entity.CheckIn</class>
			<class>cn.edu.nju.software.gof.entity.CheckInCounter</class>
			<class>cn.edu.nju.software.gof.entity.FriendRequest</class>
			<class>cn.edu.nju.software.gof.entity.OAuthAccessKey</class>
			<class>cn.edu.nju.software.gof.entity.Person</class>
			<class>cn.edu.nju.software.gof.entity.PersonalLocation</class>
			<class>cn.edu.nju.software.gof.entity.Place</class>
			<class>cn.edu.nju.software.gof.entity.Profile</class>
			<class>cn.edu.nju.software.gof.entity.RenRen</class>
			<class>cn.edu.nju.software.gof.entity.Reply</class>
			<class>cn.edu.nju.software.gof.entity.RichMan</class>
		
		<properties>
			<property name="datanucleus.NontransactionalRead" value="true" />
			<property name="datanucleus.NontransactionalWrite" value="true" />
			<property name="datanucleus.ConnectionURL" value="appengine" />
		</properties>
	</persistence-unit>

</persistence>
